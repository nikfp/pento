<section>
  <Component.hero content="Survey">
    <%= if !@demographic do %>
      <DemographicLive.Show.detail demographic={@demographic} />
    <% else %>
      <.live_component
        module={DemographicLive.Form}
        id="demographic-form"
        current_user={@current_user}
      />
    <% end %>
  </Component.hero>
</section>
